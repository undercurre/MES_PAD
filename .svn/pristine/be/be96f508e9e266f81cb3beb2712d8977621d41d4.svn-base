.page {
	position: absolute;
	top: 0px;
	right: 0px;
	bottom: 0px;
	left: 0px;
	overflow: hidden;
	background-color: #eaeaea;
}

/* custom-header */
.custom-container-full {
	position: absolute;
	top: 0px;
	right: 0px;
	bottom: 0px;
	left: 0px;
	display: -webkit-box;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-ms-flex-direction: column;
	flex-direction: column;
	overflow: hidden;
}

.custom-container-full__header {
	padding: 10px 10px;
	border-bottom: 1px solid #cfd7e5;
	background: #FFF;
}

.custom-container-full__body {
	-webkit-box-flex: 1;
	-ms-flex-positive: 1;
	flex-grow: 1;
	height: 100%;
	padding: 10px 10px;
	overflow: auto;
	position: relative;
}

.custom-container-full__footer {
	padding: 10px 10px;
	border-top: 1px solid #cfd7e5;
	background: #FFF;
}

/* custom-container-header */
.custom-container-header {
	position: relative;
	overflow: hidden;
}

.transform-cx {
	transform: translateX(0px);
	transition: transform 0.3s ease 0s;
	white-space: nowrap;
	display: table-cell;
	vertical-align: middle;
}

.cch-block {
	display: flex;
	align-items: center;
}

.el-input__inner {
	margin-bottom: 0px !important;
}

/** el-select el-input */
.el-select,
.el-input {
	margin-right: 10px;
}

.custom-form{
	width: 100%;
}
.custom-form .el-form-item__label {
	/* padding: 0 0 0 0 !important; */
}

.el-drawer__header {
	background: rgb(236, 241, 246);
	margin-bottom: 12px;
	padding: 20px !important;
}

.saoma-icon {
	display: block;
	height: 28px;
	zoom: 1.2;
	position: relative;
	margin-top: -2.5px;
}

.el-form-item--mini.el-form-item:not(:last-child),
.el-form-item--small.el-form-item:not(:last-child) {
	margin-bottom: 10px !important;
}
.el-form-item--mini.el-form-item:last-child,
.el-form-item--small.el-form-item:last-child {
	margin-bottom: 0px !important;
}

.el-drawer__body {
	overflow-y: auto;
}

/* .el-radio__label {
	display: none;
} */


.el-card__header{
	padding: 3px 1px;
}
.el-card{
	border: none;
	padding: 5px;
	font-size: 12px;
}
.el-card__body{
	padding: 3px 1px;
}
.van-picker-column{
	z-index: 99;
}
.van-picker__frame{
	background:lightblue;
}
.el-page-header__left{
	align-items: center;
}
.el-textarea__inner{
	margin-bottom: 0px;
}

.order-left-cell-button > div .el-button{
	border-radius: 0px;
}
.order-left-cell-button > div:first-child .el-button{
	border-top-right-radius: 4px!important;
}
.order-left-cell-button > div:last-child .el-button{
	border-bottom-right-radius: 4px!important;
}

.order-bottom-cell-button > .el-button{
	border-radius: 0px;
	margin: 0;
	flex: 1;
}
.order-bottom-cell-button > .el-button:first-child{
	border-top-left-radius: 4px!important;
}
.order-bottom-cell-button > .el-button:last-child{
	border-top-right-radius: 4px!important;
}

.el-row{
	margin-top: 10px;
	margin-bottom: 10px;
}
.with-scan{
	margin-right: 0;
}
.with-scan .el-input__suffix-inner{
	display: flex;
	align-items: center;
	height: 100%;
	font-size: 22px;
	color: #65a286;
}